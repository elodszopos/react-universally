{
  "name": "BoilerZone",
  "version": "11.0.1",
  "description": "A starter kit giving you the minimum requirements for a modern universal react application.",
  "main": "build/server/index.js",
  "engines": {
    "node": "6.9"
  },
  "scripts": {
    "david": "david",
    "preinstall": "node tools/scripts/preinstall",
    "clean": "babel-node tools/scripts/clean",
    "dev": "babel-node tools/development",
    "build": "babel-node tools/scripts/build",
    "analyze": "babel-node tools/scripts/analyze",
    "start": "node build/server",
    "deploy": "babel-node tools/scripts/deploy",
    "lint": "esw -c .eslintrc ./src ./tools ./test --ext '.js, .jsx' --color",
    "lint:f": "esw -c .eslintrc ./src ./tools ./test --ext '.js, .jsx' --color --fix",
    "lint:w": "esw -c .eslintrc ./src ./tools ./test --ext '.js, .jsx' --color -w",
    "lint:s": "sass-lint -c .sass-lint.yml -v -q",
    "jest": "jest --config ./jest.json",
    "j:u": "jest --config ./jest.json -u",
    "coverage": "npm run coverage:clean && npm run coverage:build",
    "coverage:clean": "node tools/scripts/coverageClean.js",
    "coverage:build": "nyc --all --cache babel-node ./tools/test/webpacklessTestRunner -- headless",
    "coverage:report": "nyc report --reporter=lcov --reporter=text-summary",
    "test": "npm run test:nowebpack",
    "t:w": "npm run test:nowebpack -- w",
    "test:watch": "npm run test:nowebpack -- w",
    "test:webpack": "NODE_ENV=test BABEL_ENV=test babel-node ./tools/test/test.entry",
    "test:webpack:w": "npm run test:webpack -- w",
    "test:w:w": "npm run test:webpack -- w",
    "test:webpack:watch": "npm run test:webpack -- w",
    "test:webpack:full": "npm run test:webpack -- browsers=Chrome,Safari,Firefox,jsdom",
    "test:w:f": "npm run test:webpack -- browsers=Chrome,Safari,Firefox,jsdom",
    "test:nowebpack": "NODE_ENV=test BABEL_ENV=test babel-node ./tools/test/webpacklessTestRunner -- headless",
    "check": "concurrently --raw \"npm run lint\" \"npm run lint:s\" \"npm run test\" \n npm run jest",
    "c:w": "concurrently --raw \"npm run lint\" \"npm run lint:s\" \"npm run jest\" && npm run t:w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elodszopos/react-universally.git"
  },
  "keywords": [
    "react",
    "boilerplate",
    "starter kit",
    "universal",
    "javascript",
    "express",
    "webpack"
  ],
  "license": "MIT",
  "dependencies": {
    "app-root-dir": "1.0.2",
    "axios": "0.15.3",
    "classnames": "^2.2.5",
    "code-split-component": "2.0.0-alpha.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "dotenv": "2.0.0",
    "express": "^5.0.0-alpha.2",
    "helmet": "3.2.0",
    "hpp": "0.2.1",
    "normalize.css": "5.0.0",
    "offline-plugin": "4.5.3",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-helmet": "3.2.3",
    "react-redux": "5.0.1",
    "react-router": "4.0.0-alpha.6",
    "react-router-addons-routes": "0.0.3",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "serialize-javascript": "1.3.0",
    "source-map-support": "^0.4.8",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.0",
    "autoprefixer": "^6.6.0",
    "babel-cli": "6.18.0",
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-plugin-transform-react-jsx-self": "6.11.0",
    "babel-plugin-transform-react-jsx-source": "6.9.0",
    "babel-preset-env": "1.1.4",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-3": "6.17.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chokidar": "^1.6.1",
    "concurrently": "^3.1.0",
    "css-loader": "0.26.1",
    "david": "^9.0.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "^2.1.14",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "glob": "7.1.1",
    "happypack": "3.0.1",
    "html-webpack-plugin": "2.24.1",
    "icepick": "^1.3.0",
    "jest-cli": "^18.0.0",
    "jsdom": "^9.9.1",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jsdom-launcher": "^5.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "match-require": "2.0.0",
    "md5": "2.2.1",
    "mocha": "^3.2.0",
    "node-notifier": "4.6.1",
    "node-sass": "4.1.1",
    "null-loader": "^0.1.1",
    "nyc": "^10.0.0",
    "postcss": "^5.2.6",
    "postcss-cssnext": "^2.9.0",
    "postcss-fixes": "^1.4.2",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.0.0",
    "postcss-load-config": "^1.0.0",
    "postcss-loader": "^1.2.0",
    "postcss-scss": "^0.4.0",
    "postcss-simple-vars": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.9.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "regenerator-runtime": "0.10.1",
    "rimraf": "2.5.4",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "semver": "5.3.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "style-loader": "0.13.1",
    "webpack": "2.2.0-rc.2",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.14.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
